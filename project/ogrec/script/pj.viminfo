	""
	" Add|remove|toggle break point here. It't done by add|remove a command line at
	" gdbcmd which will be loaded via :Cd
	" @cursor range : anywhere 
	" @param1 operType{int} : 0 add , 1 remove , 2 toggle
	" @param2 breakType{int} : 0 normal , 1 temp 
	""
	function! CbreakHere(...)
	
		let [startLine, startCol, curFile] = [line('.'), col('.'), expand('%')]|try
		
		silent execute 'edit '.s:gdbfile
		let operType = a:0 >= 1 ? 0 : a:1
		let breakType = a:0 >= 2 ? 'break' : 'tbreak'
		let cmd = breakType.' '.curFile.':'.startLine
	
		if search('\V\^'.cmd.'\$')
			if operType == 0
				echom breakType." point exists"
			else 
				normal! dd	
			endif
		else
			if operType == 1
				echom "no ".breakType." point found here"
			else	
				call append(0, cmd)
				silent execute '1,$-1sort'
			endif
		endif
		silent execute ':w'	
	
		finally|silent execute 'edit'.curFile|call cursor(startLine, startCol)|endtry
	
	endfunction
	
	""
	" echo all break point 
	""
	function! ClistBreakPoints()
	
		let [startLine, startCol, curFile] = [line('.'), col('.'), expand('%')]|try
		
		silent execute 'edit '.s:gdbfile
		let l = 0
		while l <= line('$')
			echo getline(l)	
			let l+=1
		endwhile
	
