	""
	" use g:appName, or current working dir name.
	""
	function! s:getExeName()
		if exists(g:appName)
			return g:appName	
		else
			let exefile = getcwd()
			return exefile[strridx(exefile,'/')+1:]
		endif
	endfunction
	
	""
	" call make. It use s:getExeName as default target, not all. 
	" @param1 appName : target name
	" @return : 
	""
	function! s:make(...)
		let s:makeSuccess = 1
		let target = a:0 >= 1 ? a:1 : s:getExeName()
		try
			au BufReadPost quickfix call <SID>qfBufReadPost()
			execute 'make '.target
		finally
			au! BufReadPost quickfix call <SID>qfBufReadPost()
		endtry
	endfunction
	
	function! s:qfBufReadPost()
		if search('error:')
			let s:makeSuccess = 0
		endif
	endfunction
	
	""
	" run application
	" @param1 appName : target name
	""
	function! s:execute(...)
		if !s:makeSuccess
			echom "make failed, stop execute"	
			return
		endif
		let target = a:0 >= 1 ? a:1 : s:getExeName()
		execute '!./'.target
	endfunction
	
